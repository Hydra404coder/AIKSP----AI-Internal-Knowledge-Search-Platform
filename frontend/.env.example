# =============================================================================
# FRONTEND ENVIRONMENT VARIABLES
# =============================================================================
# 
# Copy this file to .env and fill in your values.
# 
# IMPORTANT: Vite only exposes variables prefixed with VITE_
# =============================================================================

# API URL - Backend server address
# In development: http://localhost:5000 (or use proxy)
# In production: Your actual backend URL
VITE_API_URL=/api

# App name shown in UI
VITE_APP_NAME=AI Knowledge Search Platform

# Enable/disable features
VITE_ENABLE_AI_CHAT=true
